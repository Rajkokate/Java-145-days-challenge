public class JAVA {
    public static void main(String args[]) {
        System.out.print("hello ");
        System.out.print("hello java"); //simple use of print

        System.out.println("hello  raj");//for chnage next line used println
        System.out.println("hello raj");


        System.out.print("hello  raj\n");
        System.out.print("hello  raj\n");//alternate for println is "\n"
        System.out.print("hello \n raj");

    }
}
//boiler plate
